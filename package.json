    "scripts": {
        "dev-server": "encore dev-server",
        "dev": "encore dev",
        "watch": "encore dev --watch",
        "build": "encore production --progress",
        "start": "ttab -a iterm2 yarn encore dev --watch && ttab -a iterm2 symfony server:start",
        "start-code": "code . && yarn run start",
        "start-PS": "open -na \"PhpStorm.app\" && yarn run start",
        "all-install": "yarn install && composer install",
        "remove-mod": "rm -rf node_modules && rm -rf vendor",
        "remove-nodemod": "rm -rf node_modules",
        "remove-vendormod": "rm -rf vendor",
        "remove-all": "rm -rf node_modules && rm -rf vendor && rm -rf var",
        "remove-cache": "rm -rf var"
    },
